<launch>
  <!-- SVO -->
  <node pkg="svo_ros" type="svo_node" name="svo" clear_params="true" output="screen">
    <!-- Camera and IMU topics to subscribe to -->
    <param name="cam0_topic" value="/zed2/zed_node/left/image_rect_gray" type="str" />
    <param name="cam1_topic" value="/zed2/zed_node/right/image_rect_gray" type="str" />
    <param name="imu_topic" value="/zed2/zed_node/imu/data_raw" type="str" />
    <param name="calib_file" value="$(find svo_ros)/param/calib/svo_zed2_calib.yaml" type="str" />

    <rosparam file="$(find svo_ros)/param/frontend_imu/zed2_vo_param.yaml" />

  </node>

  <!-- RVIZ + SVO GUI -->
  <node name="vis" pkg="rviz" type="rviz" args=" -d $(find svo_ros)/rviz_config.rviz" />
  
</launch>
